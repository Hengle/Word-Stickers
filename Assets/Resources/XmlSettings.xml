<?xml version="1.0" encoding="UTF-8"?>

<application>
    
    <!-- rate it -->
    <!-- ios_app_id - ios application id -->
    <!-- android_app_id -  android application id -->
    <!-- coins - how many coins user get when press rate -->
    <!-- first_trigger - how many schools you need to finish to display message first time -->
    <!-- next_trigger - how many schools you need to finish to display message next time -->
    <rate_it ios_app_id="1450409320" android_app_id="com.eerylab.wordstickers" coins="100" first_trigger="1" next_trigger="5" />
    
    <!-- share -->
    <!-- coins - how many coins user get when press rate -->
    <!-- coins_num - how many times popup will have coins as an reward -->
    <!-- first_trigger - how many schools you need to finish to display message first time -->
    <!-- next_trigger - how many schools you need to finish to display message next time -->
    <share coins="100" coins_num="3" first_trigger="1" next_trigger="5" />
    
    <!-- urls -->
    <!-- type - url type: privacy, about -->
    <!-- value - url -->
    <url type="privacy" value="https://eerylab.com/privacy/" />
    <url type="about" value="https://eerylab.com/wordstickers/" />
    
    <!-- coins -->
    <!-- initial - coins at start -->
    <!-- pack - coins at pack complete -->
    <!-- perfect - coins at perfect -->
    <!-- ladybug - coins for clicking ladybug -->
    <coins initial="100" />
    <coins pack="50" />
    <coins perfect="10" />
    <coins ladybug="1" />
    
    <!--- coins in levels -->
    <!--  packs - count of packs to apply this tier -->
    <!--  b_tier_each_level - each level when b settings should be used, ex. 2 means each second level will use b settings -->
    <!--- min_coins_a - min value for random coins -->
    <!--- min_coins_a - max value for random coins -->
    <!--- min_coins_b - min value for random coins -->
    <!--- min_coins_b - max value for random coins -->
    <coins_in_levels packs="1" b_tier_each_level="2" min_coins_a="1" max_coins_a="2" min_coins_b="1" max_coins_b="3" />
    <coins_in_levels packs="2" b_tier_each_level="2" min_coins_a="1" max_coins_a="3" min_coins_b="2" max_coins_b="4" />
    <coins_in_levels packs="4" b_tier_each_level="2" min_coins_a="1" max_coins_a="3" min_coins_b="3" max_coins_b="5" />
    <coins_in_levels packs="15" b_tier_each_level="2" min_coins_a="0" max_coins_a="5" min_coins_b="3" max_coins_b="8" />
    <coins_in_levels packs="30" b_tier_each_level="2" min_coins_a="0" max_coins_a="6" min_coins_b="5" max_coins_b="10" />
